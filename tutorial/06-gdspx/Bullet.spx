onTouched => {
	destroy
}

onStart => {
	for {
		wait 0.1
		degree := 10.0
		count := 3.0
		for i:= -count+1 ; i< count ;i++{
			sprite := clone
			sprite.setXYpos mouseX, mouseY
			sprite.setYpos mouseY+30
			sprite.turnTo i * degree
		}
	}
}

onCloned => {
	show
	for {
		wait 0.01
		step 6
		if ypos >380 {
			destroy
		}
	}
}
